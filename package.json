{
  "name": "intercom-exporter-node",
  "version": "1.0.0",
  "description": "Conversation Exporter for Intercom",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "async-csv": "^2.1.3",
    "dotenv": "^16.0.3",
    "node-fetch": "^3.3.1"
  },
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint ./src"
  },
  "eslintConfig": {
    "extends": "eslint-config-strict-mode",
    "rules": {
      "require-jsdoc": "off",
      "filenames/no-index": "off",
      "no-console": "off",
      "no-await-in-loop": "off",
      "sort-keys-fix/sort-keys-fix": "off",
      "camelcase": "off",
      "max-statements": "off",
      "max-lines-per-function": "off",
      "functional/no-let": "off",
      "functional/no-loop-statement": "off",
      "fp/no-loops": "off",
      "fp/no-mutation": "off",
      "regexp/no-unused-capturing-group": "off",
      "prefer-named-capture-group": "off",
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "semi": true
        }
      ]
    }
  },
  "devDependencies": {
    "eslint-config-strict-mode": "^1.2.1",
    "prettier": "^2.8.7"
  },
  "engines": {
    "node": "^16",
    "yarn": "^1"
  }
}
